func createDirIfNotExist(path string) error {
 var err error
 baseDir := filepath.Dir(path)
 if !isExistPath(baseDir) {
  if err = os.Mkdir(baseDir, 755); err != nil {
   createDirIfNotExist(baseDir)
  }
 }
 return nil
}
```

